PROVISIONAL_LOGIN_ENABLED=true
PROVISIONAL_AUTH_ID=prov_id
PROVISIONAL_AUTH_SECRET_MASTER=prov_master
PROVISIONAL_AUTH_DOMAIN=prov_domain
REVLM_SIGV4_SECRET_KEY=test-sigv4-secret
REVLM_SIGV4_ACCESS_KEY=revlm-access
REVLM_SIGV4_REGION=revlm
REVLM_SIGV4_SERVICE=revlm
PORT=3000
